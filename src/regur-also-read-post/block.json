{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/regur-also-read-post",
	"version": "0.1.0",
	"title": "Regur Also Read Post",
	"category": "widgets",
	"icon": "smiley",
	"description": "A block to display also read posts based on the search term",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "regur-also-read-post",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"selectedPost": {
		"type": "object",
		"default": {}
		},
		"value":{
			"type":"string",
			"default": ""
		},
		"showInput":{
			"type":"boolean",
			"default": true
		},
		"editView":{
			"type":"boolean",
			"default" : false
		},
		"isLoading":{
			"type" :"boolean",
			"default" : false
		},
		"showNotFoundMsg":{
			"type": "boolean",
			"default": false
		}
	},
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
